<!--
    manifest.xml
    Jumpr - Jump Rope Tracker for Garmin Forerunner 235

    Connect IQ manifest defining app metadata, permissions, and target device.
    Targets Connect IQ 1.3 (FR235 platform).
-->
<iq:manifest xmlns:iq="http://www.garmin.com/xml/connectiq" version="3">
    <iq:application
        entry="JumpRopeApp"
        id="a1b2c3d4-e5f6-7890-abcd-ef1234567890"
        launcherIcon="@Drawables.LauncherIcon"
        minApiLevel="1.3.0"
        name="@Strings.AppName"
        type="watch-app"
        version="1.0.0">

        <iq:products>
            <iq:product id="fr235" />
        </iq:products>

        <iq:permissions>
            <iq:uses-permission id="Sensor" />
            <iq:uses-permission id="FitContributor" />
            <iq:uses-permission id="UserProfile" />
            <iq:uses-permission id="Fit" />
        </iq:permissions>

        <iq:languages>
            <iq:language>eng</iq:language>
        </iq:languages>

    </iq:application>
</iq:manifest>
